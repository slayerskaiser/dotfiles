Config {
    commands = [
            Run MultiCpu ["-t","Cpu: <total0>% <total1>% <total2>% <total3>%","-L","10","-H","80","--normal","darkorange","--high","red"] 10,
            Run Memory ["-t","Mem: <usedratio>%","-L","30","-H","90","--normal","darkorange","--high","red"] 10,
            Run Swap [] 10,
            Run Date "%a %b %-d, %Y <fc=green>%l:%M:%S %p</fc>" "date" 10,
            Run StdinReader
            ]
    , sepChar = "%"
    , alignSep = "}{"
    , template = "%StdinReader% }{ %multicpu% <fc=white>|</fc> %memory% <fc=white>|</fc> %swap% <fc=white>|</fc> %date%"
}
